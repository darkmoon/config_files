! file: ~/.Xdefaults
! author: STxza
!-------------------------------------------------------------------------------
! Xft settings

!Xft.dpi:                        96
!Xft.antialias:                  true
!Xft.hinting:                    false
!! hintnone | hintslight | hintmedium | hintfull
!Xft.hintstyle:                  hintnone
!Xft.autohint:                   false
!Xft.rgba:                       rgb
!! lcdnone | lcddefault | lcdlight | lcdlegacy
!Xft.lcdfilter:                  lcdlight
! {{{ Xcursor
Xcursor.theme: Neutral_Plus_Inverted
Xcursor.size: 10
! }}}
 
!-------------------------------------------------------------------------------
! {{{ URxvt
URxvt*font:                     xft:DejaVu Sans Mono:size=9:antialias=true,xft:WenQuanYi Bitmap Song:size=9:antialias=true
URxvt*boldFont:                 xft:DejaVu Sans Mono:size=9:antialias=true,xft:WenQuanYi Bitmap Song:size=9:antialias=true
URxvt.background:	            rgba:1111/1111/1111/dddd
!enable pseudo-transparency (requires depth: 24 (see above))
!urxvt*inheritPixmap:      true
!URxvt.depth:                    32
!URxvt.thickness:                0
URxvt.geometry:                 80x25
URxvt.loginShell:               true
URxvt*scrollBar:	            false
URxvt.transparent:              true
URxvt.urgentOnBell:             true
URxvt.visualBell:               true
URxvt.mouseWheelScrollPage:     true
URxvt.cursorBlink:              true
URxvt.cursorColor:              #dcdccc
URxvt.title:                    URxvt
URxvt.saveLines:                65535
!URxvt.borderLess:               true
!URxvt.borderWidth:              0
!URxvt.internalBorder:           0
URxvt.cutchars:                 "()*,<>[]{}|'
URxvt.secondaryScroll:          true
URxvt.perl-lib:                 /usr/lib/urxvt/perl/
URxvt.perl-ext-common:          default,matcher
URxvt.matcher.button:           1
URxvt.matcher.pattern.1:        \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.urlLauncher:              firefox
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~

! }}}

!-------------------------------------------------------------------------------
! {{{ Colours
! Zenburn color-scheme
!urxvt*background:  #3f3f3f
!urxvt*foreground:  #dcdccc
!! black + red
!!urxvt*color0:     #3f3f3f
!urxvt*color0:      #1E2320
!urxvt*color1:      #705050
!! green + yellow
!urxvt*color2:      #60b48a
!urxvt*color3:      #dfaf8f
!! blue + purple
!urxvt*color4:      #506070
!urxvt*color5:      #dc8cc3
!! cyan + white
!urxvt*color6:      #8cd0d3
!urxvt*color7:      #dcdccc
!! bright-black + bright-red
!urxvt*color8:      #709080
!urxvt*color9:      #dca3a3
!! bright-green + bright-yellow
!urxvt*color10:     #c3bf9f
!urxvt*color11:     #f0dfaf
!! bright-blue + bright-purple
!urxvt*color12:     #94bff3
!urxvt*color13:     #ec93d3
!! bright-cyan + bright-white
!urxvt*color14:     #93e0e3
!urxvt*color15:     #ffffff

*background:  #000000
*foreground:  #00ff00
*color0:     rgb:00/00/00
*color1:     rgb:e0/10/10
*color2:     rgb:20/ad/20
*color3:     rgb:d4/c2/4f
*color4:     rgb:23/1b/b8
*color5:     rgb:9c/38/85
*color6:     rgb:1d/bd/b8
*color7:     rgb:fe/fe/fe
*color8:     rgb:6a/6a/6a
*color9:     rgb:e8/3a/3d
*color10:    rgb:35/e9/56
*color11:    rgb:ff/ff/2f
*color12:    rgb:3a/53/f0
*color13:    rgb:e6/28/ba
*color14:    rgb:1c/f5/f5
*color15:    rgb:ff/ff/ff

 
! }}}
